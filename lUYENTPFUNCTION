#include <stdio.h>
char
calculateGrade (int mark)
{
  char grade;

  if (mark >= 90)
	{
	  grade = 'A';
	}
  else if (mark >= 80)
	{
	  grade = 'B';
	}
  else if (grade >= 70)
	{
	  grade = 'C';
	}
  else if (mark >= 60)
	{
	  grade = 'D';
	}
  else
	{
	  grade = 'F';
	}
  return grade;
}

int
main ()
{
  int mark;
  printf ("Enter mark: ");
  scanf ("%d", &mark);
  char grade = calculateGrade (mark);
  printf ("grade: %c\n", grade);
  return 0;
}
